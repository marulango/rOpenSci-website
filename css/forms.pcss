/* Forms */

form {

}

a.button {
    color: white;
    text-transform: uppercase;
    text-decoration: none;
    font-weight: 600;
    border-radius: 50px;
    padding: 15px 30px;
    display: inline-block;
    &.main {
        background-color: var(--main_button);
        font-size: .8rem;
    }
    &.secondary{
        background-color: var(--main_link);
        font-size: .8rem;
    }
}
